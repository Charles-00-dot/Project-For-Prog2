
public abstract class Person {
String name;
String email;
String id_num;



 public Person(String name, String email, String id_num) {
	 this.name = name;
	 this.email = email;
	 this.id_num = id_num;
 }
 
 public String toString() {
	 return "Name: " + name + "Email: " + email + "ID number: " + id_num;
 }
}
